#include <iostream>

using namespace std; 

int fibo(int n){
    if(n<2){
        return n;
    }
    else{
    return fibo(n-2)+fibo(n-1);
}
} 


int main(){
    int a,n; //a = position of element in fibonachi series n= term at ath position
   cin>>n;
    for(int i =0; i<=n; i++){
    cout<<fibo(i);
    }
    return 0;
